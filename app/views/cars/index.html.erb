<h1>Welcome to GA Car DB</h1>

<%@cars.each do |car|%>
<table>
<tr>
  <th>Make</th>
  <td><%= car.make %></td>
<tr>
  <th>Model name</th>
  <td><%= car.model_name %></td>
<tr>
  <th>Color</th>
  <td><%= car.color %></td>
<tr>
  <th>Year</th>
  <td><%= car.year %></td>
<tr>
  <th>Doors</th>
  <td><%= car.doors %></td>
<tr>
  <th>Description</th>
  <td><%= car.description %></td>
<tr>
  <th>Image</th>
  <td><%= car.image_url %></td>
</table>
<br><br>
<%end%>

